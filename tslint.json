{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": [true, "always-prefix"],
        "array-type": [true, "generic"],
        "trailing-comma": [
            false,
            {
              "multiline": {
                "objects": "always",
                "arrays": "always",
                "functions": "never",
                "typeLiterals": "ignore"
              },
              "esSpecCompliant": true
            }
          ]
    },
    "rulesDirectory": []
}